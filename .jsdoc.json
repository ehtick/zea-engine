{
  "opts": {
    "destination": "docs/",
    "inheritance-diagram": {
      "node": {
        "dimensions": {
          "width": 250
        }
      }
    },
    "recurse": true,
    "template": "node_modules/ink-docstrap/template"
  },
  "plugins": ["node_modules/jsdoc-inheritance-diagram"],
  "source": {
    "include": ["src/", "README.md"]
  },
  "templates": {
    "includeDate": false,
    "outputSourceFiles": false,
    "outputSourcePath": true,
    "systemName": "Zea Engine",
    "theme": "cerulean"
  }
}
