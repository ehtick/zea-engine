{
  "name": "@zeainc/zea-engine",
  "fileName": "index",
  "libraryName": "ZeaEngine",
  "version": "1.1.0",
  "description": "Zea Engine",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "author": "Zea Inc.",
  "license": "LicenseRef-LICENSE",
  "files": [
    "/LICENSE",
    "/dist/index.cjs.js",
    "/dist/index.esm.js",
    "/public-resources/**",
    "/buildApiDocs.js",
    "/setup-visual-test.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c rollup.config.dev.js -w",
    "prepare": "yarn run build",
    "dist": "yarn publish --access=public",
    "docs": "adg --config adg.config.json",
    "docs-w": "adg -w --config=adg.config.json",
    "testing-server": "node testing-server/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:watch": "jest --watch",
    "test:visual": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test:visual:watch": "karma start --auto-watch --browsers ChromeHeadless karma.conf.js",
    "test:visual:debug": "karma start --auto-watch --single-run karma.conf.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@zeainc/jsdocs2md": "^0.0.3",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "ink-docstrap": "^1.3.2",
    "jest": "^26.1.0",
    "jest-coverage-badges": "^1.1.2",
    "karma": "^5.0.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "mocha": "^7.1.2",
    "prettier": "^2.0.2",
    "resemblejs": "^3.2.4",
    "rollup": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-web-worker-loader": "^1.3.0",
    "yargs": "3.32.0",
    "@babel/preset-env": "^7.10.4"
  }
}
