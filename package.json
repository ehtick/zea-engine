{
  "name": "@zeainc/zea-engine",
  "fileName": "index",
  "libraryName": "ZeaEngine",
  "version": "1.2.0",
  "description": "Zea Engine",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "author": "Zea Inc.",
  "license": "SEE LICENSE IN LICENSE.md",
  "files": [
    "/dist/index.cjs.js",
    "/dist/index.esm.js",
    "/public-resources/**",
    "/buildApiDocs.js",
    "/setup-visual-test.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c rollup.config.dev.js -w",
    "prepare": "yarn run build",
    "dist": "yarn publish --access=public",
    "docs": "cp CHANGELOG.md docs/ && adg --config adg.config.json",
    "docs-w": "cp CHANGELOG.md docs/ && adg -w --config=adg.config.json",
    "release": "standard-version",
    "start": "cp -R dist/ testing-e2e/dist/ && es-dev-server -r testing-e2e",
    "testing-server": "node testing-server/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:watch": "jest --watch",
    "test:e2e": "percy exec cypress run --browser chrome --headless",
    "test:e2e:watch": "percy exec cypress open",
    "to-cleanup": "rm -Rf dist/ node_modules/ yarn.lock"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@percy/cypress": "^2.3.1",
    "@zeainc/jsdocs2md": "^0.0.3",
    "chai": "^4.2.0",
    "cypress": "^4.12.1",
    "es-dev-server": "^1.57.2",
    "eslint": "^6.5.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "ink-docstrap": "^1.3.2",
    "jest": "^26.1.0",
    "jest-coverage-badges": "^1.1.2",
    "karma": "^5.0.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "mocha": "^7.1.2",
    "prettier": "^2.0.2",
    "resemblejs": "^3.2.4",
    "rollup": "^2.2.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-web-worker-loader": "^1.3.0",
    "standard-version": "^8.0.2"
  }
}
