  "version": "0.0.82",
  "description": "Zea Engine",
  "main": "dist/zea-engine.js",
  "author": "Zea Inc.",
  "license": "ISC",
  "files": [
    "/dist/zea-engine.js",
    "/public-resources/**"
  ],
  "scripts": {
    "build:dev": "webpack --watch --config webpack-dev.config.js",
